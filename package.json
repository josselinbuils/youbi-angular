{
  "name": "youbi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-node && npm run build-browser",
    "build-browser": "ng build --prod",
    "build-lib": "npm install ../naudiodon && npm run postinstall",
    "build-node": "tsc -p ./src/node/tsconfig.json",
    "lint": "npm run lint-browser && npm run lint-node",
    "lint-browser": "tslint -c ./src/browser/tslint.json -p ./src/browser/tsconfig.json ./src/browser/**/*.ts",
    "lint-node": "tslint -c ./src/node/tslint.json -p ./src/node/tsconfig.json ./src/node/**/*.ts",
    "package-mac": "node ./scripts/package darwin",
    "package-win": "node ./scripts/package win32",
    "postinstall": "electron-rebuild",
    "start": "npm run build-node && electron ."
  },
  "main": "dist/node/main.js",
  "private": true,
  "dependencies": {
    "aac": "^0.1.3",
    "alac": "^0.1.0",
    "ansicolor": "^1.1.71",
    "av": "^0.4.9",
    "color-thief-browser": "^2.0.2",
    "electron-store": "^1.3.0",
    "electron-window-state": "^4.1.1",
    "flac.js": "^0.2.1",
    "fs-extra": "^5.0.0",
    "ipc-promise": "^0.1.4",
    "moment": "^2.20.1",
    "mp3": "^0.1.0",
    "music-metadata": "^0.9.5",
    "naudiodon": "git://github.com/josselinbuils/naudiodon.git",
    "ogg.js": "^0.1.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sharp": "^0.19.1",
    "through2": "^2.0.3",
    "vorbis.js": "^0.1.1"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.6",
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.6",
    "@angular/core": "^5.2.6",
    "@angular/forms": "^5.2.6",
    "@angular/http": "^5.2.6",
    "@angular/platform-browser": "^5.2.6",
    "@angular/platform-browser-dynamic": "^5.2.6",
    "@angular/router": "^5.2.6",
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "^5.2.6",
    "@angular/language-service": "^5.2.6",
    "@types/electron": "^1.6.10",
    "@types/node": "~9.4.6",
    "codelyzer": "^4.1.0",
    "core-js": "^2.5.3",
    "electron": "^1.8.2",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.7.3",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.5",
    "rxjs": "^5.5.6",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.20"
  }
}
